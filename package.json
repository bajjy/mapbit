{
  "name": "@mapbit/osm-rpg-converter",
  "version": "1.0.0",
  "description": "Convert OpenStreetMap vector objects to RPG-style pixel art maps",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "server:dev": "cd server && npm run dev",
    "client:dev": "cd client && npm run dev",
    "build": "cd client && npm run build",
    "start": "cd server && npm start",
    "install:all": "npm install && cd server && npm install && cd ../client && npm install",
    "test:orthogonalize": "node test-orthogonalize.js"
  },
  "keywords": ["osm", "rpg", "map", "pixel-art", "leaflet", "vector"],
  "author": "MapBit",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2",
    "node-fetch": "^3.3.2"
  }
}
